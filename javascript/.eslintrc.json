{
  "extends": "google",
  "env": {
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "max-len": [ "error", { "code": 120, "tabWidth": 2 } ],
    "require-jsdoc": "warn",
    "comma-dangle": [ "error", "never" ],
    "padded-blocks": [ "error", { "switches": "never" } ],
    "no-warning-comments": [ "warn" ]
  }
}
